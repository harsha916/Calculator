<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="add" class="com.spring.cal.beans.Add" scope="prototype">
		<property name="num1" value="25" />
		<property name="num2" value="25" />
	</bean>
	<bean id="sub" class="com.spring.cal.beans.Sub" scope="prototype">
		<property name="num1" value="100" />
		<property name="num2" value="75" />
	</bean>
	<bean id="mul" class="com.spring.cal.beans.Mul" scope="prototype">
		<property name="num1" value="5" />
		<property name="num2" value="5" />
	</bean>
	<bean id="div" class="com.spring.cal.beans.Div" scope="prototype">
		<property name="num1" value="40" />
		<property name="num2" value="4" />
	</bean>

</beans>